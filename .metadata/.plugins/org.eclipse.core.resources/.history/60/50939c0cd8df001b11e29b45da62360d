package chapter05;

public class PersonTest {

	public static void main(String[] args) {
		Person p = new Person();
		p.setAge(40);
		p.setName("James");
		p.setMarriage(true);
		p.setKid(3);
		
		System.out.println(p.getName() + "의 나이는 " + p.getAge() + "세이고, " + p.isMarriage() + "이며, 자녀의 수는 " + p.getKid() + "명입니다.");
	}

}
